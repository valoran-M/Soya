class point {
  attribute int x;
  attribute int y;
  method void constructor(int x, int y) {
    this.x = x;
    this.y = y;
  }
  method int sum(int x) {
    return (x+this.x+this.y);
  }
}

class point3 extends point {
  attribute int z;

  method void constructor(int x, int y, int z) {
    super.constructor(x, y);
    this.z = z;
  }

  method int sum(int x) {
    return super.sum(x) + this.z;
  }
}

class point4 extends point3 {
  attribute int w;

  method void constructor(int x, int y, int z, int w) {
    super.constructor(x, y, z);
    this.w = w;
  }

  method int sum(int x) {
    return super.sum(x) + this.w;
  }
}

function void main() {
  var point4 p;
  var int c;
  p = new point4(1, 1, 2, 1);
  c = p.sum(48);
  putchar(c);
}

// prints '3'

